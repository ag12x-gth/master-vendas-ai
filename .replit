modules = ["nodejs-20", "bash", "web", "nix", "go-1.21", "go-1.24", "postgresql-16"]
[agent]
expertMode = true
integrations = ["javascript_object_storage:1.0.0", "replitmail:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["postgresql_17", "chromium", "glib", "nspr", "nss", "dbus", "atk", "cups", "cairo", "pango", "mesa", "xorg.libX11", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXext", "xorg.libXfixes", "xorg.libXrandr", "xorg.libxcb", "libxkbcommon", "at-spi2-core", "at-spi2-atk", "alsa-lib", "libgbm", "imagemagick"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Production Server"

[[workflows.workflow]]
name = "Production Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run start:prod"
waitForPort = 8080

[[ports]]
localPort = 8080
externalPort = 8080

[[ports]]
localPort = 44729
externalPort = 80

[deployment]
deploymentTarget = "vm"
run = ["npm", "run", "start:prod"]
build = ["npm", "run", "build"]

[objectStorage]
defaultBucketID = "replit-objstore-5c7db24d-3f1a-48f3-a419-34323e1b5779"
