{
  "name": "nextn",
  "version": "2.4.2",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5000 --hostname 0.0.0.0",
    "dev:server": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' tsx watch server.js",
    "dev:safe": "bash scripts/auto-fix-server.sh && npm run dev:server",
    "dev:clean": "set NODE_OPTIONS=--no-deprecation && next dev -p 5000 --hostname 0.0.0.0",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' SKIP_LINT=false next build",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=4096' SKIP_LINT=true SKIP_TYPECHECK=true next build",
    "build:prod": "next build",
    "start": "next start -p 5000",
    "start:prod": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' node server.js",
    "lint": "eslint . --ext .ts,.tsx --cache --cache-location .next/cache/eslint",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "fix": "bash scripts/auto-fix-server.sh",
    "fix:restart": "bash scripts/auto-fix-server.sh && npm run dev:server",
    "health": "bash scripts/health-check.sh",
    "db:generate": "drizzle-kit generate",
    "db:generate:vector": "drizzle-kit generate --config=drizzle.vector.config.ts",
    "db:migrate": "tsx scripts/migrate-db.ts",
    "db:migrate:vector": "tsx scripts/migrate-vector-db.ts",
    "db:push": "yes '' | drizzle-kit push --config=drizzle.config.ts",
    "db:rollback": "tsx scripts/rollback-db.ts",
    "db:reset": "rm -rf drizzle && npm run db:generate && npm run db:migrate",
    "test:queue": "tsx scripts/seed-test-campaigns.ts",
    "seed:templates": "tsx src/scripts/seed-predefined-templates.ts",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --fix-type problem,suggestion,layout"
  },
  "dependencies": {
